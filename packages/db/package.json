{
  "name": "@digital-inv/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "main": "src/index.ts",
  "scripts": {
    "generate": "prisma generate --schema prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema prisma/schema.prisma",
    "studio": "prisma studio --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "db:seed": "prisma db seed --schema prisma/schema.prisma",
    "db:nuke": "tsx prisma/nuke.ts",
    "db:nuke:hard": "prisma db nuke --schema prisma/schema.prisma",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0"
  },
  "devDependencies": {
    "prisma": "6.15.0",
    "typescript": "^5.6.2"
  }
}
